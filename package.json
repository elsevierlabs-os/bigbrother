{
  "name": "bigbrother",
  "version": "0.4.1",
  "author": "Marco Stagni <m.stagni@elsevier.com>",
  "description": "Performance Testing tool",
  "bugs": {
    "url": "https://github.com/SDFE/sd-browser-perf/issues"
  },
  "homepage": "https://github.com/SDFE/sd-browser-perf",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "build": "npm run build:lib && npm run build:reports",
    "build:lib": "npm run clean && babel src --out-dir dist",
    "build:reports": "webpack --verbose --colors --display-error-details --config reports.webpack.config.js",
    "clean": "rimraf -rf ./dist",
    "preversion": "npm run test",
    "version": "npm run build && git add dist",
    "postversion": "git push --no-verify && git push --tags --no-verify",
    "start": "npm run build && ./index.js --config=./dev.bigbrother.config.js '**/*.perf.js'",
    "start:file": "npm run build && ./index.js --config=./dev.bigbrother.config.js",
    "start:noconfig": "npm run build && ./index.js '**/*.perf.js'",
    "start:nonheadless": "npm run build && HEADLESS=false ./index.js --config=./dev.bigbrother.config.js '**/*.perf.js'"
  },
  "bin": {
    "bigbrother": "index.js"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/runtime": "7.7.7",
    "chart.js": "^1.1.1",
    "colors": "1.3.2",
    "command-line-usage": "6.0.2",
    "glob": "7.1.4",
    "lodash.flattendeep": "^4.4.0",
    "minimist": "1.2.5",
    "node-fetch": "2.6.0",
    "object-inspect": "1.6.0",
    "ora": "3.4.0",
    "puppeteer": "1.7.0",
    "react": "^16.13.1",
    "react-chartjs": "^1.2.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "recharts": "^1.8.5",
    "replace-in-file": "^6.0.0",
    "rimraf": "2.6.3",
    "safe-eval": "0.4.1",
    "tree-kill": "1.2.2",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/register": "7.9.0",
    "@babel/polyfill": "7.8.7",
    "autoprefixer": "^9.7.6",
    "babel-loader": "8.0.6",
    "css-loader": "^3.5.3",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.3",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  }
}
