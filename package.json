{
  "name": "bigbrother",
  "version": "0.5.0",
  "author": "Marco Stagni <m.stagni@elsevier.com>",
  "description": "Performance Testing tool",
  "bugs": {
    "url": "https://github.com/SDFE/sd-browser-perf/issues"
  },
  "homepage": "https://github.com/SDFE/sd-browser-perf",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "test:ut": "NODE_ENV=test mocha --reporter mocha-better-spec-reporter ./config/test.config.js 'src/**/*.test.@(js|jsx)'",
    "test": "npm run test:coverage",
    "test:watch": "npm run test:ut -- --watch",
    "test:coverage": "nyc --reporter=lcov --reporter=text npm run test:ut",
    "build": "npm run build:lib && npm run build:reports",
    "build:lib": "npm run clean && babel --config-file ./config/babel.config.js src --out-dir dist",
    "build:reports": "webpack --verbose --colors --display-error-details --config ./config/reports.webpack.config.js",
    "clean": "rimraf -rf ./dist",
    "preversion": "npm run test",
    "version": "npm run build && git add dist",
    "postversion": "git push --no-verify && git push --tags --no-verify",
    "start": "npm run build && BIGBROTHER_LOCAL=true ./index.js --config=./config/dev.bigbrother.config.js '**/*.perf.js'",
    "start:file": "npm run build && ./index.js --config=./dev.bigbrother.config.js",
    "start:noconfig": "npm run build && ./index.js '**/*.perf.js'",
    "start:nonheadless": "npm run build && HEADLESS=false ./index.js --config=./config/dev.bigbrother.config.js '**/*.perf.js'",
    "lint": "eslint -c ./config/.eslintrc.js --max-warnings=0 src",
    "lint:fix": "eslint -c ./config/.eslintrc.js --max-warnings=0 src --fix",
    "prettier": "prettier --write src"
  },
  "bin": {
    "bigbrother": "index.js"
  },
  "pre-push": [
    "test"
  ],
  "nyc": {
    "include": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/runtime": "7.7.7",
    "babel-eslint": "10.0.2",
    "chart.js": "^1.1.1",
    "colors": "1.3.2",
    "command-line-usage": "6.0.2",
    "glob": "7.1.4",
    "lodash.flattendeep": "^4.4.0",
    "minimist": "1.2.5",
    "mocha": "^8.1.0",
    "node-fetch": "2.6.1",
    "object-inspect": "1.6.0",
    "ora": "3.4.0",
    "puppeteer": "1.20.0",
    "react": "^16.13.1",
    "react-chartjs": "^1.2.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "recharts": "^1.8.5",
    "replace-in-file": "^6.0.0",
    "rimraf": "2.6.3",
    "tree-kill": "1.2.2",
    "yargs": "13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-export-default-from": "7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/preset-react": "7.10.1",
    "@babel/register": "7.10.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "8.0.6",
    "chai": "^4.2.0",
    "chai-jest-snapshot": "^2.0.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "7.2.0",
    "eslint-plugin-mocha": "7.0.1",
    "eslint-plugin-react": "^7.20.6",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha-better-spec-reporter": "^3.1.0",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "postcss-loader": "^3.0.0",
    "pre-push": "^0.1.1",
    "prettier": "2.0.5",
    "proxyquire": "^2.1.3",
    "sass-loader": "^7.0.3",
    "sinon": "^9.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11"
  }
}
